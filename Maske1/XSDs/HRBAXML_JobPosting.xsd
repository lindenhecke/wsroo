							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="0" />
									<xs:maxInclusive value="1" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Objective" minOccurs="0" maxOccurs="1">
