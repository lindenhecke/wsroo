<?xml version="1.0" encoding="UTF-8"?>
<!-- Bundesagentur fuer Arbeit - Schema fÃ¼r den Im- und Export von Stellenangeboten ueber HRBAXML-->
<!-- Version: 1.1 / Verbis PRV 15.1 -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="HRBAXML.xsd" />
	<xs:element name="HRBAXMLJobPositionPosting">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Header" type="HeaderType" />
				<xs:element name="Data">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="JobPositionPosting" type="JobPositionPostingType"
								minOccurs="0" maxOccurs="unbounded" />
							<xs:element name="DeleteEntry" type="DeleteEntryType"
								minOccurs="0" maxOccurs="unbounded" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="JobPositionPostingType">
		<xs:sequence>
			<xs:element name="JobPositionPostingId">
				<xs:annotation>
					<xs:documentation>+++ BESCHREIBUNG: Hier vergeben Sie fuer den
						jeweiligen Datensatz eine eindeutige ID. Es wird eine fortlaufende
						Nummerierung empfohlen, zusaetzlich sollte die Jahreszahl
						ersichtlich sein. Fuer Aktualisierung / Loeschung wird diese ID
						wieder benoetigt.
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1" />
						<xs:maxLength value="27" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="HiringOrg">
				<xs:annotation>
					<xs:documentation>+++ BESCHREIBUNG: Angaben zum Arbeitgeber
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="HiringOrgName">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Name des Arbeitgebers
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1" />
									<xs:maxLength value="90" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="HiringOrgId" minOccurs="0">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Es handelt sich um ein
									wichtiges Pflichtfeld fuer Arbeitgeber und wird mit der
									Kundennummer des Arbeitgebers bzw. der
									Arbeitgeber-Niederlassung befuellt. Die Kundennummer bzw.
									Kundennummern werden Ihnen auf Anfrage vom Account- und
									Allianzmanagement der BA mitgeteilt. E-Mail-Adresse:
									Zentrale.Vam-AccountMgmt@arbeitsagentur.de
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="10" />
									<xs:maxLength value="10" />
									<xs:pattern
										value="[ABCEFGHJKLMNPQRSTUVWXYZ]{1}[0-9]{3}[ABCDEFGHJKLMNPQRSTUVWXY]{1}[0-9]{5}" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="ProfileWebSite" minOccurs="0">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Internetpraesenz des
									Unternehmens.
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="0" />
									<xs:maxLength value="80" />
									<xs:pattern value="(http|https|ftp)(://)[\w\W]+" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="HiringOrgSize" minOccurs="0">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Unternehmensgroesse +++
									ATTRIBUTE: weniger als 6(1) , zwischen 6 und 50(2) , zwischen
									51 und 500(3) , zwischen 501 und 5000(4) , zwischen 5001 und
									50000(5) , mehr als 50000(6)
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="1" />
									<xs:maxInclusive value="6" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Industry" minOccurs="0">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Angaben zur Branche
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="NAICS" minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Branche des Arbeitgebers
												+++ ATTRIBUTE: Moegliche Werte sind dem zu Grunde liegenden
												Katalog zu entnehmen (WZ2008) der im Outverzeichnis der
												Software Connect:Direct dem jeweiligen Kooperationspartners
												zur Verfuegung gestellt wird
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:pattern value="(([0-9]{1,2}\.{1})*[0-9]{1,2}|[A-Z]{1})" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="Contact">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Kontaktdaten des
									Arbeitgebers
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Salutation" minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Anrede +++ ATTRIBUTE:
												Herr(1) , Frau(2)
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:minInclusive value="1" />
												<xs:maxInclusive value="2" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="Title" minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Titel</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="15" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="GivenName" minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Vorname
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="30" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="NamePrefix" minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Vorsatzwort
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="30" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="FamilyName">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Familienname
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="1" />
												<xs:maxLength value="30" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="PositionTitle" minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Angaben zur Position im
												Unternehmen
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="40" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="PostalAddress" type="AddressType"
										minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Adresse des Arbeitgebers
											</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="VoiceNumber" type="TelecommuncationType"
										minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Telefonnummer
											</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="MobilNumber" type="TelecommuncationType"
										minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Mobilnummer
											</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="FaxNumber" type="TelecommuncationType"
										minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Faxnummer
											</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="EMail" minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: E-Mail Adresse fuer
												Kontakt
												mit
												dem Arbeitgeber
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="254" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<xs:element name="GeneralWebSite" minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Internetpraesenz des
												Arbeitgebers. Verweist auf die Homepage des Arbeitgebers
											</xs:documentation>
										</xs:annotation>
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:minLength value="0" />
												<xs:maxLength value="80" />
												<xs:pattern value="(http|https|ftp)(://)[\w\W]+" />
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PostDetail">
				<xs:annotation>
					<xs:documentation>+++ BESCHREIBUNG: Weitere Angaben zum
						Stellenangebot
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="StartDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Beginn der Veroeffentlichung
									des Stellenangebots
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EndDate" type="xs:date" minOccurs="0">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Ende der Veroeffentlichung
									des
									Stellenangebots
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LastModificationDate" type="xs:dateTime">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Datum der letzten Aenderung
									an
									diesem Stellenangebot
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Status">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Angaben zum
									Veroeffentlichungsstatus des Stellenangebots. Die Status
									"intern
									veroeffentlicht"(6), "intern anonym veroeffentlicht"(7),
									"intern
									und Jobboerse anonym veroeffentlicht"(8) sind reine
									Exportstatus,
									die ausschliesslich an zugelassene kommunale
									Traeger gemeldet
									werden. +++ ATTRIBUTE: veroeffentlicht(1),
									anonym
									veroeffentlicht(2), nicht veroeffentlicht(3), intern
									veroeffentlicht(6), intern anonym veroeffentlicht(7), intern
									und
									Jobboerse anonym veroeffentlicht(8)
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:enumeration value="1" />
									<xs:enumeration value="2" />
									<xs:enumeration value="3" />
									<xs:enumeration value="6" />
									<xs:enumeration value="7" />
									<xs:enumeration value="8" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Aktion">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Angaben zur Uebermittlung
									+++
									ATTRIBUTE: Neuanlage(1), Aktualisierung(2), Loeschung(3)
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="1" />
									<xs:maxInclusive value="3" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="SupplierId">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Ihre ID - diese wird Ihnen
									nach Abschluss der Kooperationsvereinbarung mitgeteilt.
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1" />
									<xs:maxLength value="10" />
									<xs:pattern value="[A|P|V|K][0-9]{9}" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="SupplierName">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Name des Anbieters von dem
									das
									Stellenangebot bezogen wird
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="1" />
									<xs:maxLength value="45" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="SupplierIndustrie" minOccurs="0">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Art des Anbieters +++
									ATTRIBUTE: Allgemeine Stellenboersen (1) , Regionale
									Stellenboersen (2) , Berufs- und branchenspez. Stellenboersen
									(3)
									, Sonstige spezielle Stellenboersen (4) , Arbeitgeber(5)
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:integer">
									<xs:minInclusive value="1" />
									<xs:maxInclusive value="6" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="InternetReference" minOccurs="0">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Link auf die Detailansicht
									des
									zugehoerigen Stellenangebots auf der InternetprÃ¤senz des
									Anbieters (DeepLink)
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:minLength value="0" />
									<xs:maxLength value="200" />
									<xs:pattern value="(http|https|ftp)(://)[\w\W]+" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="PostedBy" minOccurs="0">
							<xs:annotation>
								<xs:documentation>+++ BESCHREIBUNG: Angaben zum Anbieter des
									Stellenangebots
								</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Contact" minOccurs="0">
										<xs:annotation>
											<xs:documentation>+++ BESCHREIBUNG: Kontaktdaten des
												Anbieters
											</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:sequence>
												<xs:element name="Company" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Firmenname
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="90" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Salutation" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Anrede +++ ATTRIBUTE:
															Herr (1), Frau (2)
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:integer">
															<xs:minInclusive value="1" />
															<xs:maxInclusive value="2" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Title" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Titel
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="15" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="GivenName" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Vorname
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="30" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="NamePrefix" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Vorsatzwort
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string" />
													</xs:simpleType>
												</xs:element>
												<xs:element name="FamilyName" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Familienname
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="30" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="PositionTitle" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Angaben zur Position
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="40" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="PostalAddress" type="AddressType"
													minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Postanschrift
														</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="VoiceNumber" type="TelecommuncationType"
													minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Telefonnummer
														</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="MobilNumber" type="TelecommuncationType"
													minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Mobilnummer
														</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="FaxNumber" type="TelecommuncationType"
													minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Faxnummer
														</xs:documentation>
													</xs:annotation>
												</xs:element>
												<xs:element name="EMail" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: E-Mail des Anbieters
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="254" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="JobContactWebSite" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Internetpraesenz des
															Stellenangebots. Verweist auf die Onlinebewerbungsseite
															fuer dieses Stellenangebot beim Anbieter
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="0" />
															<xs:maxLength value="200" />
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="InterviewContact" minOccurs="0">
													<xs:annotation>
														<xs:documentation>+++ BESCHREIBUNG: Alternativer
															Ansprechpartner
														</xs:documentation>
													</xs:annotation>
													<xs:complexType>
														<xs:sequence>
															<xs:element name="Salutation" minOccurs="0">
																<xs:annotation>
																	<xs:documentation>+++ BESCHREIBUNG: Anrede +++
																		ATTRIBUTE:
																		Herr(1) , Frau(2)
																	</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:integer">
																		<xs:minInclusive value="1" />
																		<xs:maxInclusive value="2" />
																	</xs:restriction>
																</xs:simpleType>
															</xs:element>
															<xs:element name="Title" minOccurs="0">
																<xs:annotation>
